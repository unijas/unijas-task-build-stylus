{
  "stylus": {
    "main": [
      ["src", "stylesheets", "**/*.styl"]
    ],
    "includes": [
      ["src", "app", "components", "**", "styles", "*.styl"],
      ["src", "stylesheets", "lib", "**", "*.styl"]
    ],
    "dest": [
      ["dist", "public", "css"]
    ],
    "conf": {
      "compress": true,
      "use": ["nib", "rupture", "jeet"],
      "import": ["nib", "jeet"]
    }
  }
}
